<%= semantic_form_for :manual_input, :url => do_manual_input_admin_lentil_images_path do |form| %>
    <%= form.input :urls, :as => :text, :label => "Instagram URLs (one per line)", :input_html => { :class => 'expanding', :rows => 10, :cols => 30 } %>
    <%= form.actions do %>
      <%= form.action :submit, :label => "Import images" %>
    <% end %>
<% end -%>
